<h1>Listing etapas</h1>

<table>
  <thead>
    <tr>
      <th>Etapa1</th>
      <th>Indicador1</th>
      <th>Etapa2</th>
      <th>Indicador2</th>
      <th>Etapa3</th>
      <th>Indicador3</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @etapas.each do |etapa| %>
      <tr>
        <td><%= etapa.etapa1 %></td>
        <td><%= etapa.indicador1 %></td>
        <td><%= etapa.etapa2 %></td>
        <td><%= etapa.indicador2 %></td>
        <td><%= etapa.etapa3 %></td>
        <td><%= etapa.indicador3 %></td>
        <td><%= link_to 'Show', etapa %></td>
        <td><%= link_to 'Edit', edit_etapa_path(etapa) %></td>
        <td><%= link_to 'Destroy', etapa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Etapa', new_etapa_path %>
